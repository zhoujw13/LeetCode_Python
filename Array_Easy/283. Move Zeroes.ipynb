   ],
   "source": [
    "nums = [0,1,0,3,12]\n",
    "n= len(nums)\n",
    "k = 0\n",
    "for i in range(n):\n",
    "    if nums[i] !=0:\n",
    "        nums[k] = nums[i]\n",
    "        k= k+1\n",
    "nums[k:]=[0,]*(n-k)\n",
    "nums"
   ]
